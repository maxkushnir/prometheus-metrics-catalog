{
  "services": [
    {
      "name": "databases",
      "displayName": "Databases",
      "description": "Database and data store metrics",
      "path": "/data/services/databases/metrics.json",
      "color": "blue",
      "isGroup": true,
      "subservices": [
        {
          "name": "aerospike",
          "displayName": "Aerospike",
          "description": "NoSQL database metrics",
          "exporter": {
            "name": "aerospike-prometheus-exporter",
            "link": "https://github.com/NewRelic/aerospike-prometheus-exporter"
          }
        },
        {
          "name": "redis",
          "displayName": "Redis",
          "description": "In-memory data structure store metrics",
          "exporter": {
            "name": "redis_exporter",
            "link": "https://github.com/oliver006/redis_exporter"
          }
        },
        {
          "name": "mysql",
          "displayName": "MySQL",
          "description": "MySQL database server metrics",
          "exporter": {
            "name": "mysqld_exporter",
            "link": "https://github.com/prometheus/mysqld_exporter"
          }
        },
        {
          "name": "mongodb",
          "displayName": "MongoDB",
          "description": "MongoDB database server metrics",
          "exporter": {
            "name": "mongodb_exporter",
            "link": "https://github.com/percona/mongodb_exporter"
          }
        },
        {
          "name": "postgresql",
          "displayName": "PostgreSQL",
          "description": "PostgreSQL database server metrics",
          "exporter": {
            "name": "postgres_exporter",
            "link": "https://github.com/prometheus-community/postgres_exporter"
          }
        }
      ]
    },
    {
      "name": "kafka",
      "displayName": "Kafka",
      "description": "Apache Kafka streaming platform metrics",
      "path": "/data/services/kafka/metrics.json",
      "color": "indigo",
      "exporter": {
        "name": "kminion",
        "link": "https://github.com/cloudhut/kminion"
      }
    },
    {
      "name": "monitoring",
      "displayName": "System Monitoring",
      "description": "System and application monitoring metrics",
      "path": "/data/services/monitoring/metrics.json",
      "color": "green",
      "isGroup": true,
      "subservices": [
        {
          "name": "node_exporter",
          "displayName": "Node Exporter",
          "description": "System and hardware metrics",
          "exporter": {
            "name": "node_exporter",
            "link": "https://github.com/prometheus/node_exporter"
          }
        },
        {
          "name": "jmx_exporter",
          "displayName": "JMX Exporter",
          "description": "Java application metrics via JMX",
          "exporter": {
            "name": "jmx_exporter",
            "link": "https://github.com/prometheus/jmx_exporter"
          }
        }
      ]
    },
    {
      "name": "nginx",
      "displayName": "Nginx",
      "description": "Web server and reverse proxy metrics",
      "path": "/data/services/nginx/metrics.json",
      "color": "orange",
      "exporter": {
        "name": "nginx-prometheus-exporter",
        "link": "https://github.com/nginxinc/nginx-prometheus-exporter"
      }
    },
    {
      "name": "pure",
      "displayName": "Pure Storage",
      "description": "FlashArray and FlashBlade storage metrics",
      "path": "/data/services/pure/metrics.json",
      "color": "purple",
      "isGroup": true,
      "subservices": [
        {
          "name": "pure_flasharray",
          "displayName": "FlashArray",
          "description": "FlashArray storage metrics",
          "exporter": {
            "name": "pure-fa-openmetrics-exporter",
            "link": "https://github.com/PureStorage-OpenConnect/pure-fa-openmetrics-exporter"
          }
        },
        {
          "name": "pure_flashblade", 
          "displayName": "FlashBlade",
          "description": "FlashBlade storage metrics",
          "exporter": {
            "name": "pure-fb-openmetrics-exporter",
            "link": "https://github.com/PureStorage-OpenConnect/pure-fb-openmetrics-exporter"
          }
        }
      ]
    }
  ]
}
